class ZObject abstract{
    protected
    void Error(String Msg)
    {Cl_Util.Log(GetClassName(), LOG_Error, Msg);}

    protected
    void Fatal(String Msg)
    {Cl_Util.Log(GetClassName(), LOG_Fatal, Msg);}
}

class ZThinker:Thinker abstract{
    protected
    void Error(String Msg)
    {Cl_Util.Log(GetClassName(), LOG_Error, Msg);}

    protected
    void Fatal(String Msg)
    {Cl_Util.Log(GetClassName(), LOG_Fatal, Msg);}
}

