class ZF_Operator:ZF_Base abstract{
    ZF_Base Main,
            Operand;

    static
    ZF_Operator Create(Name Type, ZF_Base Main, ZF_Base Operand){
        ZF_Operator Result=ZF_Operator(ZF_Base.Create('O'..Type));
        Result.Main=Main;
        Result.Operand=Operand;
        return Result;
    }

    override
    ZF_Base Clone() const{
        ZF_Operator Result=ZF_Operator(super.Clone());
        Result.Main=Main.Clone();
        Result.Operand=Operand.Clone();
        return Result;
    }
}