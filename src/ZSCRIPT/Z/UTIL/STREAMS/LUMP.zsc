class ZS_Lump:ZS_String{
    static
    ZS_Lump Create(uint Lump, Name Type='Lump')
    {return ZS_Lump(ZS_String.Create(Wads.ReadLump(Lump), Type));}

    /////////////
    // Writing //
    /////////////

    override
    void Write(String Data)
    {Fatal("Cannot write to lumps");}

    override
    void Append(String Data)
    {Fatal("Cannot write to lumps");}
}